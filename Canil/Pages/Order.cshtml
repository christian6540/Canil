@page "{reference}"
@model Canil.Pages.OrderModel
@{
    Layout = "/Views/Shared/_LayoutLojaUser.cshtml";
}

<div>
    <h1>
        @Model.Order.OrderRef
    </h1>

    @foreach (var product in Model.Order.Products)
    {
        <div>
            @product.Name - @product.Description - @product.Qty - @product.Value
        </div>
    }
</div>